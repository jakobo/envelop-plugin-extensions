{
  "name": "envelop-plugin-extensions",
  "description": "Set extensions via your GraphQL Context",
  "version": "0.0.1",
  "author": "Jakob Heuser",
  "license": "MIT",
  "homepage": "https://github.com/jakobo/envelop-plugin-extensions/",
  "repository": "jakobo/envelop-plugin-extensions.git",
  "scripts": {
    "build": "run-s tsup",
    "clean": "shx rm -rf dist && shx mkdir dist",
    "eslint": "eslint",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "rel": "release-it",
    "sp": "syncpack",
    "test": "ava",
    "tsup": "tsup",
    "typecheck": "tsc --noEmit"
  },
  "tsup": {
    "entry": {
      "index": "src/index.ts"
    },
    "format": [
      "esm",
      "cjs"
    ],
    "dts": true,
    "splitting": false,
    "sourcemap": true,
    "clean": true,
    "minify": false
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "package.json",
    "README.md",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14.19.0",
    "pnpm": "7.x"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@envelop/core": "^3.0.6",
    "@envelop/testing": "^5.0.6",
    "@envelop/types": "^3.0.2",
    "@graphql-tools/schema": "^9.0.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.14.2",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "ava": "^5.2.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "graphql": "^16.6.0",
    "husky": "^7.0.4",
    "lint-staged": "13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.0",
    "release-it": "^15.5.0",
    "shx": "^0.3.4",
    "syncpack": "^7.2.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3"
  },
  "peerDependencies": {
    "@envelop/core": "3.*",
    "graphql": "*"
  },
  "keywords": [
    "envelop",
    "envelop-plugin",
    "graphql",
    "plugin",
    "yoga"
  ]
}
